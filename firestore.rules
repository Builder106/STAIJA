rules_version = "2";nservice cloud.firestore {n  match /databases/{database}/documents {n    // Allow authenticated users to read/write their own datan    match /users/{userId} {n      allow read, write: if request.auth != null && request.auth.uid == userId;n    }n    n    // Allow staff to read all applicationsn    match /applications/{applicationId} {n      allow read, write: if request.auth != null && n        (request.auth.uid == resource.data.userId || n         request.auth.token.email.matches(".*@staija\.org"));n    }n    n    // Allow staff to read/write programsn    match /programs/{programId} {n      allow read: if true;n      allow write: if request.auth != null && n        request.auth.token.email.matches(".*@staija\.org");n    }n    n    // Allow authenticated users to create contact messagesn    match /contact_messages/{messageId} {n      allow read: if request.auth != null && n        request.auth.token.email.matches(".*@staija\.org");n      allow create: if true;n    }n  }n}